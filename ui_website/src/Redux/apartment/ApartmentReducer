import {
    FETCH_APARTMENT,
    FETCH_ROOM,
    FETCH_FEE
} from './ApartmentType';

const initialState = {
    apartment: []
};

const ApartmentReducer = (state = initialState, action) => {
    switch (action.type) {
        case FETCH_APARTMENT :
        return {
            ...state,
            apartment: action.payload,
        };

        // case FETCH_SINGLE_BILL :
        // return {
        //     ...state,
        //     singleBill : action.payload
        // };

        // case CREATE_BILL :
        // return {
        //     ...state,
        //     newBill : action.payload
        // };



        default:
            return state;
        }
      };

export default ApartmentReducer;